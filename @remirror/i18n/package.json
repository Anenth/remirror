{
  "name": "@remirror/i18n",
  "version": "0.0.0",
  "description": "Supported internationalization and locales for the remirror editor.",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/next/@remirror/i18n",
  "repository": "https://github.com/remirror/remirror/tree/next/@remirror/i18n",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "main": "dist/i18n.cjs.js",
  "module": "dist/i18n.esm.js",
  "types": "dist/i18n.cjs.d.ts",
  "files": [
    "dist",
    "plurals",
    "en"
  ],
  "dependencies": {
    "@babel/runtime": "^7.10.0",
    "@lingui/core": "^3.0.0-13",
    "@remirror/core-helpers": "^0.7.6",
    "make-plural": "^6.2.1"
  },
  "devDependencies": {
    "@remirror/pm": "^0.0.0"
  },
  "peerDependencies": {
    "@remirror/pm": "^0.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "50 KB"
  },
  "preconstruct": {
    "entrypoints": [
      ".",
      "plurals",
      "en"
    ]
  }
}
