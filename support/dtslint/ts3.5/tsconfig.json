{
  "extends": "../../tsconfig.paths.json",
  "compilerOptions": {
    "jsx": "preserve",
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"]
  }
}
