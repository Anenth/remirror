{
  "name": "@remirror/extension-code-block",
  "version": "2.0.0-beta.0",
  "description": "Unleash the inner coder with code blocks for your remirror editor.",
  "keywords": [
    "remirror",
    "remirror-extension",
    "code",
    "code block",
    "refractor",
    "prism",
    "editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-code-block",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-code-block"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./formatter": {
      "import": "./formatter/dist/remirror-extension-code-block-formatter.js",
      "default": "./formatter/dist/remirror-extension-code-block-formatter.js"
    },
    ".": {
      "default": "./dist/remirror-extension-code-block.js",
      "import": "./dist/remirror-extension-code-block.js"
    }
  },
  "main": "./dist/remirror-extension-code-block.js",
  "module": "./dist/remirror-extension-code-block.js",
  "browser": {
    "./dist/remirror-extension-code-block.cjs.js": "./dist/remirror-extension-code-block.browser.cjs.js",
    "./dist/remirror-extension-code-block.esm.js": "./dist/remirror-extension-code-block.browser.esm.js"
  },
  "types": "./dist/remirror-extension-code-block.cjs.d.ts",
  "files": [
    "formatter",
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^2.0.0-beta.0",
    "@remirror/messages": "^2.0.0-beta.0",
    "@remirror/theme": "^2.0.0-beta.0",
    "@types/refractor": "^3.0.2",
    "refractor": "^3.3.1"
  },
  "devDependencies": {
    "@remirror/pm": "^2.0.0-beta.0",
    "@types/prettier": "^2.3.2",
    "prettier": "^2.3.2"
  },
  "peerDependencies": {
    "@remirror/pm": "^2.0.0-beta.0",
    "@types/prettier": "^2",
    "prettier": "^2"
  },
  "peerDependenciesMeta": {
    "@types/prettier": {
      "optional": true
    },
    "@types/refractor": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "20 KB"
  },
  "preconstruct": {
    "entrypoints": [
      "formatter.ts",
      "index.ts"
    ]
  },
  "rn:dev": "src/index.ts"
}
