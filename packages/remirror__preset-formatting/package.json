{
  "name": "@remirror/preset-formatting",
  "version": "0.0.0",
  "private": true,
  "description": "A preset with all the formatting node and mark extensions included.",
  "keywords": [
    "remirror",
    "preset",
    "text-editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__preset-formatting",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__preset-formatting",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-preset-formatting.esm.js",
      "require": "./dist/remirror-preset-formatting.cjs.js",
      "browser": "./dist/remirror-preset-formatting.browser.esm.js",
      "types": "./dist/remirror-preset-formatting.cjs.d.ts",
      "default": "./dist/remirror-preset-formatting.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-preset-formatting.cjs.js",
  "module": "dist/remirror-preset-formatting.esm.js",
  "browser": {
    "./dist/remirror-preset-formatting.cjs.js": "./dist/remirror-preset-formatting.browser.cjs.js",
    "./dist/remirror-preset-formatting.esm.js": "./dist/remirror-preset-formatting.browser.esm.js"
  },
  "types": "dist/remirror-preset-formatting.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@remirror/core": "1.0.0-next.60",
    "@remirror/pm": "1.0.0-next.60"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "10 KB"
  }
}
